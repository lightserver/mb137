<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="ch.dofour.web" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>dufour-web</name>
  <description>
    Add your desription here
  </description>
  <author email="email@example.com">
    css team
  </author>
  <content src="index.html"/>
  <access origin="*"/>
